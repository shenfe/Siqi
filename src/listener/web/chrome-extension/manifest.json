{
    "name": "siqi",
    "description": "web ai, started from voice control",
    "version": "1.0.0",
    "devtools_page": "devtools.html",
    "permissions": ["activeTab"],
	"background": {
		"scripts": [
			"background.js"
		]
	},
    "content_scripts": [
        {
            "matches": ["https://*.focus.cn/*", "http://127.0.0.1:8001/*", "http://sogou.com/*"],
            "js": ["contentScript.js"],
            "run_at": "document_start"
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png",
        "19": "icon19.png",
        "38": "icon38.png"
    },
    "content_security_policy": "default-src 'self' chrome-extension-resource: ; img-src 'self' data:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval'; connect-src *; frame-src *;",
    "manifest_version": 2
}
